@model KluczToSukcesDoKariery.Models.MaterialEdu

<h1>Potwierdzenie usunięcia</h1>

<p>Czy na pewno chcesz usunąć materiał: <strong>@Model.Tytul</strong>?</p>

<form method="post">
    <input type="hidden" name="id" value="@Model.Id" />
    <button type="submit" class="btn btn-danger">Usuń</button>
    <a href="@Url.Action("Index")" class="btn btn-secondary">Anuluj</a>
</form>